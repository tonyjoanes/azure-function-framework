<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2012/06/nuspec.xsd">
  <metadata>
    <id>AzureFunctionFramework</id>
    <version>1.0.0</version>
    <title>Azure Function Framework</title>
    <authors>Azure Function Framework Team</authors>
    <owners>Azure Function Framework Team</owners>
    <description>A lightweight NuGet package to simplify Azure Functions development in .NET. It abstracts away boilerplate for common triggers (HTTP, Service Bus), configuration (App Config + Key Vault), and environment variable setup.</description>
    <summary>Simplify Azure Functions development with reduced boilerplate and configuration management</summary>
    <language>C#</language>
    <projectUrl>https://github.com/your-org/azure-function-framework</projectUrl>
    <repository type="git" url="https://github.com/your-org/azure-function-framework" />
    <license type="expression">MIT</license>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <copyright>Copyright 2025</copyright>
    <tags>azure functions dotnet framework boilerplate configuration app-config key-vault service-bus http</tags>
    <packageTypes>
      <packageType name="DotnetTool" />
    </packageTypes>
    <dependencies>
      <group targetFramework=".NET8.0">
        <dependency id="Microsoft.Azure.Functions.Worker" version="1.23.0" />
        <dependency id="Microsoft.Extensions.Configuration" version="8.0.0" />
        <dependency id="Microsoft.Extensions.Configuration.AzureAppConfiguration" version="6.5.0" />
        <dependency id="Microsoft.Extensions.Configuration.AzureKeyVault" version="3.1.1" />
        <dependency id="Microsoft.Extensions.DependencyInjection" version="8.0.0" />
        <dependency id="Microsoft.Extensions.Hosting" version="8.0.0" />
        <dependency id="Microsoft.Extensions.Logging" version="8.0.0" />
        <dependency id="Microsoft.Azure.ServiceBus" version="5.2.0" />
        <dependency id="Azure.Identity" version="1.10.4" />
        <dependency id="System.Text.Json" version="8.0.0" />
      </group>
    </dependencies>
  </metadata>
  <files>
    <file src="README.md" target="" />
  </files>
</package>
